@charset "UTF-8";

@import 'customizations';   // color presets and cover image

@import 'compass';          // The Compass framework

@import "compass/css3/images"; 
@import "compass/utilities/general/hacks";


@import 'reset';            // jonikorpi's custom CSS reset
@import 'type';             // typography presets and tools
@import 'media-queries';    // media query presets

/*
  This line has now ss-pika as default in order to get it loaded in Heroku.
  If you want to run in Heroku with font-awesome, you'll need to change this
  (e.g. by replacing the whole line with font-awesome.min import) 
*/

<%= (APP_CONFIG.icon_pack == "font-awesome" ? "@import 'font-awesome.min';" : "@import 'ss-social';\n@import 'ss-pika';").html_safe %>

/* Partials */

@import "partials/grid";
@import "partials/map";

/* OLD main stylesheet

   This file is deprecated: Put your SCSS to views/ and partials/ 
   and include them in application.scss.erb
*/

@import 'main';

/* Views */
@import "views/listings";
@import "views/people";
@import "views/testimonial";
