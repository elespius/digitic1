{
  "repository": "sharetribe/sharetribe",
  "license": "MIT",
  "engines": {
    "node": "5.10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "build:client": "webpack --config webpack.client.rails.build.config.js",
    "build:server": "webpack --config webpack.server.rails.build.config.js",
    "build:production:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:dev:client": "webpack -w --config webpack.client.rails.build.config.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "hot-assets": "babel-node server-rails-hot.js",
    "print-phantomjs-version": "phantomjs --version",
    "start-phantomjs": "phantomjs --webdriver=8910"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel": "6.5.2",
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "css-loader": "0.23.1",
    "es5-shim": "4.5.8",
    "es6-promise": "3.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "loader-utils": "0.2.14",
    "lodash": "4.11.1",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.5.1",
    "postcss-loader": "0.8.2",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-on-rails": "5.2.0",
    "react-redux": "4.4.5",
    "redux": "3.5.1",
    "redux-thunk": "2.0.1",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "sass-resources-loader": "1.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.3",
    "babel-plugin-react-transform": "2.0.2",
    "body-parser": "1.15.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-react": "4.3.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "phantomjs-prebuilt": "2.1.7",
    "react-transform-hmr": "1.0.4",
    "webpack-dev-server": "1.14.1"
  },
  "browser": {
    "fs": false
  }
}
