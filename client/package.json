{
  "repository": "sharetribe/sharetribe",
  "license": "MIT",
  "engines": {
    "node": "5.10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "build:test": "webpack --config webpack.client.js && webpack --config webpack.server.js",
    "test": "npm run build:test && rspec",
    "build:production:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:dev:client": "webpack -w --config webpack.client.rails.build.config.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "hot-assets": "babel-node server-rails-hot.js"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "css-loader": "0.23.1",
    "es5-shim": "4.5.8",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "history": "2.0.1",
    "imports-loader": "0.6.5",
    "loader-utils": "0.2.14",
    "lodash": "4.9.0",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.2",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-on-rails": "5.2.0",
    "react-redux": "4.4.2",
    "react-router": "2.0.1",
    "redux": "3.4.0",
    "redux-thunk": "2.0.1",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "sass-resources-loader": "1.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.15"

  },
  "devDependencies": {
    "babel-plugin-react-transform": "2.0.2",
    "body-parser": "1.15.0",
    "babel-eslint": "6.0.2",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.3.0",
    "react-transform-hmr": "1.0.4",
    "webpack-dev-server": "1.14.1"
  },
  "browser": {
    "fs": false
  }
